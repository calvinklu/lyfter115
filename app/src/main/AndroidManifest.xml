<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="lifter.app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity"></activity>
        <activity android:name=".ProfilePageActivity"></activity>
        <activity android:name=".AboutMeActivity"></activity>
        <activity
            android:name=".MySchedule"
            android:label="@string/my_schedule"></activity>
        <activity android:name=".AddTimeActivity"></activity>
        <activity android:name=".MyWorkoutsActivity"></activity>
        <activity
            android:name=".Sidebar"
            android:label="@string/title_activity_sidebar"
            android:theme="@style/AppTheme.NoActionBar" />

    </application>

</manifest>